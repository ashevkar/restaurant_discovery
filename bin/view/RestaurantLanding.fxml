<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RestaurantLandingController">
   <children>
      <AnchorPane prefHeight="486.0" prefWidth="702.0">
         <children>
            <ImageView fitHeight="494.0" fitWidth="711.0" opacity="0.81" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Desktop/Restaurant/food4.png" />
               </image>
            </ImageView>
            <Label alignment="TOP_CENTER" layoutX="5.0" layoutY="7.0" prefHeight="47.0" prefWidth="417.0" text="ADMIN PANEL" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="40.0" />
               </font>
            </Label>
            <Button fx:id="addrestaurant" layoutX="108.0" layoutY="106.0" mnemonicParsing="false" onAction="#addRestaurantAction" prefHeight="47.0" prefWidth="212.0" text="Add Restaurant">
               <font>
                  <Font name="Arial Bold" size="17.0" />
               </font>
            </Button>
            <Button fx:id="adminlogout" layoutX="14.0" layoutY="447.0" mnemonicParsing="false" onAction="#gotoLogin" prefHeight="27.0" prefWidth="117.0" text="Log Out">
               <font>
                  <Font size="13.0" />
               </font></Button>
            <Button fx:id="updateRestaurant" layoutX="108.0" layoutY="189.0" mnemonicParsing="false" onAction="#updateRestaurantAction" prefHeight="47.0" prefWidth="212.0" text="Update Restaurant">
               <font>
                  <Font name="Arial Bold" size="17.0" />
               </font>
            </Button>
            <Button fx:id="deleteRestaurant" layoutX="108.0" layoutY="277.0" mnemonicParsing="false" onAction="#deleteRestaurantAction" prefHeight="47.0" prefWidth="212.0" text="Delete Restaurant">
               <font>
                  <Font name="Arial Bold" size="17.0" />
               </font>
            </Button>
            <Label fx:id="landinglabel" alignment="CENTER" layoutX="16.0" layoutY="412.0" prefHeight="18.0" prefWidth="118.0" text="" textAlignment="CENTER" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
